{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "off",
    "consistent-return": "off",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "camelcase": "off",
    "no-plusplus": "off",
    "no-param-reassign": "warn",
    "eqeqeq": "warn",
    "no-restricted-syntax": "off"
  }
}
